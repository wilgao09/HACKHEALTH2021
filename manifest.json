{
    "name": "HealthHack Project Name",
    "version": "1.0",
    "description": "Name",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [        "tabs",
        "<all_urls>",
        "notifications",
        "http://*/",
        "https://*/",
        "storage"
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
        },
        "default_title": "Where do THESE words go????"
    },
    "content_scripts": [
        {
        "matches": [
                "<all_urls>",
                "http://*/*",
                "https://*/*",
                "file://*/*"
            ],
        "js": ["contentListener.js", "messageProcessor.js"],
        "run_at": "document_end"   
        }
    ], 
    "icons": {
    },
    "manifest_version": 2
}
